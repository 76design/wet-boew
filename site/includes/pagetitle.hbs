{{title}}{{#isnt assets ".."}}
	{{#contains page.src "src/"}}
		{{#i18n "hyphen"}}{{/i18n}}{{#i18n "tmpl-work-examples"}}{{/i18n}}
		{{#is breadcrumb3 "true"}}
			{{#i18n "hyphen"}}{{/i18n}}{{breadcrumb3-title}}
		{{/is}}
	{{/contains}}
	{{#contains page.src "/docs/versions/"}}
		{{#contains page.src "/index"}}
		{{else}}
			{{#i18n "hyphen"}}{{/i18n}}{{#i18n "tmpl-version-history"}}{{/i18n}}
		{{/contains}}
	{{else}}
		{{#contains page.src "/docs/"}}
			{{#i18n "hyphen"}}{{/i18n}}{{#i18n "tmpl-documentation"}}{{/i18n}}
			{{#contains page.src "/gs-cd/"}}
				{{#contains page.src "/index"}}
				{{else}}
					{{#i18n "hyphen"}}{{/i18n}}{{#i18n "tmpl-getting-started"}}{{/i18n}}
				{{/contains}}
			{{/contains}}	
			{{#if page.componentType}}
				{{#i18n "hyphen"}}{{/i18n}}{{page.componentType}}
			{{/if}}
		{{/contains}}
	{{/contains}}
{{/isnt}}{{#i18n "hyphen"}}{{/i18n}}{{#i18n "site.title"}}{{/i18n}}
